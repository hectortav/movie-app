{
    "name": "controller",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "nodemon -w src --ext ts --exec ts-node-esm src/index.ts",
        "build": "tsc --project ./",
        "start": "node dist/index.js",
        "lint": "eslint . --ext .js,.ts --fix",
        "test:integration": "jest int",
