        "helmet": "^6.0.0",
        "ioredis": "^5.2.3",
        "model": "*",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "@types/connect-redis": "^0.0.18",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/express-session": "^1.17.5",
        "@types/jest": "^29.1.2",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.12",
        "@types/redis": "^4.0.11",
        "@types/supertest": "^2.0.12",
        "concurrently": "^7.4.0",
        "eslint": "7.32.0",
        "eslint-config-custom": "*",
        "jest": "^29.1.2",
        "nodemon": "^2.0.20",
        "supertest": "^6.3.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsconfig": "*",
        "validation-n-types": "*",
        "typescript": "^4.5.3"
