{
    "name": "model",
    "version": "0.0.0",
    "private": true,
    "exports": {
        ".": "./dist"
    },
    "scripts": {
        "dev": "tsc -w",
        "build": "tsc --project .",
        "start": "",
        "lint": "eslint . --ext .js,.ts --fix",
        "generate": "npx prisma migrate dev --name init",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "test:integration": "yarn docker:up && (./scripts/prismaRunTestMigrations.sh && jest -c jest.config.int.js int); ret_code=$?; yarn docker:down; exit $ret_code",
        "test:unit": "jest -c jest.config.unit.js unit",
        "test": "concurrently \"yarn test:unit\" \"yarn test:integration\""
    },
    "dependencies": {
        "@prisma/client": "^4.4.0",
        "argon2": "^0.29.1",
        "dotenv": "^16.0.3",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/argon2": "^0.15.0",
        "@types/jest": "^29.1.1",
        "@types/node": "^17.0.12",
        "@types/uuid": "^8.3.4",
        "concurrently": "^7.4.0",
        "eslint": "7.32.0",
        "eslint-config-custom": "*",
        "jest": "^29.1.2",
        "jest-mock-extended": "^3.0.1",
        "prisma": "^4.4.0",
        "ts-jest": "^29.0.3",
        "tsconfig": "*",
        "typescript": "^4.5.3"
    }
}
